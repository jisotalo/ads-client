  },
  "keywords": [
    "ADS",
    "twincat",
    "beckhoff",
    "plc",
    "iec-61131-3",
    "61131-3",
    "twincat 2",
    "twincat 2.11",
    "twincat 3",
    "twincat 3.1",
    "twincat ADS",
    "codesys",
    "client"
  ],
  "author": "Jussi Isotalo <j.isotalo91@gmail.com> (https://github.com/jisotalo)",
  "license": "MIT",
  "homepage": "https://github.com/jisotalo/ads-client/",
  "bugs": {
    "url": "https://github.com/jisotalo/ads-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jisotalo/ads-client.git"
  },
  "dependencies": {
    "debug": "^4.3.3",
    "iconv-lite": "^0.6.3",
    "long": "^5.2.0"
  },
  "devDependencies": {
    "docdash": "^1.2.0",
    "jest": "^28.1.3",
